// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}
model customer{
  name String
  email_id String @unique
  phone_number Int
  customer_id Int @unique @id
  age Int
  address String
}
